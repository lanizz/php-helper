{
  "name": "lanizz/php-helper",
  "description": "PHP工具集",
  "keywords": [
    "php","helper","tools"
  ],
  "homepage": "http://github.com/lanizz/php-helper",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Jinming Lan",
      "email": "lanjinming@hotmail.com",
      "homepage": "http://blog.lanizz.com"
    }
